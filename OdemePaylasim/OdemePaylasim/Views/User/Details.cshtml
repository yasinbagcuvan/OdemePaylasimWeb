@using OdemePaylasim.Data.Entities
@model User

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>@Model.UserName Detay bilgisi</h4>
    <hr />
    <dl class="row">
        
        <dt class="col-sm-10">
            <label>UserName :</label>  @Html.DisplayFor(model => model.UserName)
        </dt>
        <dd class="col-sm-10">
            <label>Paid Money :</label>    @Html.DisplayFor(model => model.PaidMoney)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a class="btn btn-danger" asp-action="Index"> BACK </a>
</div>